{
  "colors": {
    "editor.foreground": "#0A0A0A",
    "foreground": "#0A0A0A",
    "tab.activeForeground": "#0A0A0A",
    "editorLineNumber.activeForeground": "#B9B9B9",
    "sideBar.foreground": "#0A0A0A",
    "list.activeSelectionBackground": "#0A0A0A",
    "list.inactiveSelectionForeground": "#0A0A0A",
    "activityBar.foreground": "#0A0A0A",
    "badge.foreground": "#0A0A0A",
    "activityBarBadge.foreground": "#0A0A0A",
    "sideBar.border": "#0A0A0A",
    "activityBar.border": "#0A0A0A",
    "statusBar.background": "#0A0A0A",
    "editorGroup.border": "#2D2D2D",
    "tab.activeBorder": "#2D2D2D",
    "editorLineNumber.foreground": "#2D2D2D",
    "activityBar.inactiveForeground": "#2D2D2D",
    "editor.lineHighlightBorder": "#B9B9B9",
    "list.inactiveSelectionBackground": "#B9B9B9",
    "list.activeSelectionForeground": "#B9B9B9",
    "activityBarBadge.background": "#B9B9B9",
    "badge.background": "#B9B9B9",
    "titleBar.activeBackground": "#DCDCDC",
    "tab.inactiveBackground": "#DCDCDC",
    "editor.background": "#FFFFFF",
    "editorGroupHeader.tabsBackground": "#FFFFFF",
    "tab.border": "#FFFFFF",
    "activityBar.background": "#FFFFFF",
    "sideBar.background": "#FFFFFF"
  },
  "tokenColors": [
    {
      "name": "Regular",
      "scope": [
        "keyword",
        "storage",
        "keyword.operator",
        "variable",
        "storage.type.function.arrow",
        "constant",
        "meta.template.expression",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "meta.function-call.arguments",
        "support.type.property-name"
      ],
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Italic",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.block.preprocessor",
        "comment.documentation",
        "comment.block.documentation",
        "string.quoted.docstring",
        "keyword.control.conditional",
        "keyword.control.trycatch",
        "keyword.control.switch",
        "support",
        "storage.type",
        "constant.language",
        "variable.language",
        "variable.parameter.function.language.special",
        "string",
        "meta.tag entity.other.attribute-name",
        "entity.other.attribute - name.html",
        "keyword.control.from",
        "markup.italic",
        "punctuation.definition.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Bold",
      "scope": [
        "support.function",
        "entity.name.function",
        "meta.function-call",
        "entity.name.type",
        "entity.other.inherited-class",
        "punctuation.accessor",
        "entity.name.tag",
        "support.class.component.tsx",
        "support.class.component.jsx",
        "keyword.operator.arithmetic",
        "keyword.control.import",
        "markup.bold",
        "punctuation.definition.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Underline",
      "scope": [
        "keyword.control.flow",
        "markup.underline",
        "punctuation.definition.underline"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Bold Underline",
      "scope": [
        "markup.heading",
        "punctuation.definition.heading"
      ],
      "settings": {
        "fontStyle": "bold underline"
      }
    }
  ],
  "name": "Paper",
  "type": "light"
}